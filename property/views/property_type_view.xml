<odoo>
	<data>
		<record id="property_type_view" model="ir.ui.view">
            <field name="name">type</field>
            <field name="model">property.type</field>
            <field name="arch" type="xml">
                <form string="Tipos de propiedades">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="property_id">
                        <tree string="Propiedades" multi_edit="1">
                            <field name="name"/>
                            <field name="expected_price"/>
                            <field name="active"/>
                            <field name="state" widget="statusbar" statusbar_visible="sold, cancel"/>
                        </tree>
                    </field>
                </form>

            </field>
        </record>
	</data>
</odoo>